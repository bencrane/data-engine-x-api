# Core database + platform connectivity
DATA_ENGINE_DATABASE_URL=
DATA_ENGINE_SUPABASE_URL=
DATA_ENGINE_SUPABASE_SERVICE_KEY=

# Base URL of the FastAPI service. FastAPI uses this for Trigger task payloads and
# Trigger.dev runtime uses it for internal callbacks/execution requests.
DATA_ENGINE_API_URL=https://api.dataengine.run

# Trigger.dev project/auth configuration
DATA_ENGINE_TRIGGER_SECRET_KEY=
DATA_ENGINE_TRIGGER_PROJECT_ID=
DATA_ENGINE_TRIGGER_API_URL=https://api.trigger.dev

# Auth / JWT
DATA_ENGINE_JWT_SECRET=
DATA_ENGINE_SUPER_ADMIN_JWT_SECRET=

# Super-admin API key (optional, alternative to JWT for super-admin endpoints)
# If set, this value is accepted as Bearer token on /api/super-admin/* endpoints.
DATA_ENGINE_SUPER_ADMIN_API_KEY=

# Internal service auth (Trigger.dev -> FastAPI)
# Shared secret used as Bearer token for /api/internal/* and internal /api/v1/execute calls.
DATA_ENGINE_INTERNAL_API_KEY=

# Trigger.dev runtime required variables (must exist in Trigger environment):
# - DATA_ENGINE_API_URL
# - DATA_ENGINE_INTERNAL_API_KEY

# Provider API keys
DATA_ENGINE_ICYPEAS_API_KEY=
DATA_ENGINE_LEADMAGIC_API_KEY=
DATA_ENGINE_MILLIONVERIFIER_API_KEY=
DATA_ENGINE_REOON_API_KEY=
DATA_ENGINE_PARALLEL_API_KEY=
DATA_ENGINE_PROSPEO_API_KEY=
DATA_ENGINE_BLITZAPI_API_KEY=
DATA_ENGINE_COMPANYENRICH_API_KEY=
DATA_ENGINE_ADYNTEL_API_KEY=
DATA_ENGINE_ADYNTEL_EMAIL=

# Provider ordering (operation waterfalls)
DATA_ENGINE_COMPANY_ENRICH_PROFILE_ORDER=prospeo,blitzapi,companyenrich,leadmagic
DATA_ENGINE_COMPANY_SEARCH_ORDER=prospeo,blitzapi,companyenrich
DATA_ENGINE_PERSON_SEARCH_ORDER=prospeo,blitzapi,companyenrich
DATA_ENGINE_PERSON_RESOLVE_MOBILE_ORDER=leadmagic,blitzapi

# LLM routing + keys
DATA_ENGINE_LLM_PRIMARY_MODEL=gemini
DATA_ENGINE_LLM_FALLBACK_MODEL=gpt-4
DATA_ENGINE_GEMINI_API_KEY=
DATA_ENGINE_OPENAI_API_KEY=

# Runtime parameters
DATA_ENGINE_PARALLEL_PROCESSOR=core
DATA_ENGINE_ICYPEAS_POLL_INTERVAL_MS=2000
DATA_ENGINE_ICYPEAS_MAX_WAIT_MS=45000
